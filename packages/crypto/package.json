{
  "name": "@trading-bot/crypto",
  "version": "0.0.0",
  "private": true,
  "description": "Enterprise-grade cryptographic operations for trading bot platform",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "index.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .tsbuildinfo",
    "type-check": "tsc --noEmit",
    "lint": "eslint index.ts",
    "test": "echo 'Tests not implemented yet' && exit 0"
  },
  "keywords": [
    "crypto",
    "encryption",
    "jwt",
    "bcrypt",
    "security",
    "trading-bot"
  ],
  "devDependencies": {
    "@types/node": "^20",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "latest"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "jose": "^6.0.11",
    "tslib": "^2.6.2",
    "bip39": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}